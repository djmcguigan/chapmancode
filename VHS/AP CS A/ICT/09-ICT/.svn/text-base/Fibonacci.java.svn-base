public class Fibonacci
{
    public static int fib(int num)
    {
        if(num == 0)
        {
            return 0;
        }
        else if(num == 1)
        {
            return 1;
        }
        else 
        {
            return fib(num - 1) + fib(num-2);
        }
    }
    
    public static int mult(int num1, int num2)
    {
        if((num1 == 0) || (num2 == 0))
        {
            return 0;
        }
        else if (num1 == 1)
        {
            return num2; 
        }
        else if(num2 == 1)
        {
            return num1;
        }
        else
        {
            return (num1 + mult(num1 , num2 - 1));
        }
    }
}


